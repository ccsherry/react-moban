// 此文件是通过脚本生成的，直接编辑无效！！！
<% for(let i = 0; i< routes.length; i++){ %>import <%= `Component` + i %> from '<%= routes[i].path%>';
<%}%>

// 不需要导航框架的页面路径
export const noFrames = [<% for(let i = 0; i < noFrames.length; i++){ %>
    '<%= noFrames[i] %>',<% } %>
];

// 不需要登录就可以访问的页面路径
export const noAuths = [<% for(let i = 0; i < noAuths.length; i++){ %>
    '<%= noAuths[i] %>',<% } %>
];

// 需要keep alive 页面
export const keepAlives = [<% for(let i = 0; i < keepAlives.length; i++){ %>
    {
        path: '<%= keepAlives[i].path %>',
        keepAlive: <%= keepAlives[i].keep %>,
    },<% } %>
];

// 页面路由配置
export default [<% for(let i = 0; i< routes.length; i++){ %>
    {
        path: '<%= routes[i].routePath%>',
        component: <%= `Component` + i %>,
    },<% } %>
];
